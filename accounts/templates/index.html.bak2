<!--Style begin-->
<style>
  p{
    text-align: justify;
    text-justify: auto;
  }
    h1{   
      display: inline-block;
      color: white;
      font-size: 58px;
      text-shadow: .03em .03em 0 black;
      transition: all 0.3s;
      }

      h1:hover {
        transform: scale(1.1);
      }
      
     h1:after {
    content: attr(data-shadow);
    position: absolute;
    top: .06em; left: .06em;
    z-index: -1;
    text-shadow: none;
    background-image:
      linear-gradient(
        45deg,
        transparent 35%,
        black 75%,
        black 95%,
        transparent 0
        );
    background-size: .05em .05em;
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
  
    animation: shad-anim 20s linear infinite;
    }

@keyframes shad-anim {
  0% {background-position: 0 0}
  0% {background-position: 100% -100%}
  }


  .type-text{    
  color: white;
  font-family: "arial";
  font-size: 24px;
  display: grid;
  place-items: center;
  width: 650px;
  }

.typed {
  overflow: hidden;
  white-space: nowrap;
  border-right: 2px solid;
  width: 0;
  animation: typing 1.5s steps(30, end) forwards, blinking 1s infinite;
}
  
@keyframes typing {
  from { width: 0 }
  to { width: 100% }
}

@keyframes blinking {
  0% {border-color: transparent}
  50% {border-color:  white}
  100% {border-color: transparent}
}

.register-now{
  
  text-align:left;
  padding-left:17px;

}
.register-now:before{
  content:"â†’";
  position:absolute;
  color: #4bb8c2;
  left: 83%;
  opacity: 0;
  -webkit-transition: all 250ms cubic-bezier(0.680, -0.550, 0.265, 1.550); 
}

.register-now:hover:before{
  left:84%;
  opacity:1;
}

.register-now:hover {
  width:170px;
}

.image-background{
  width: 100%;
  height: 700px;
  background-image: url(https://artifacts.ai/wp-content/uploads/2020/07/Open-Science-2.jpg);
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

</style>

